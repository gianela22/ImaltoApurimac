setwd("H:/Gore Cusco/IMA/pi alto apurimac/proyecto/encuesta")
datos <- read.csv("respuestas.csv")
datos$Comunidad <- trimws(datos$Comunidad)


#para eliminar los espacios en blanco de
datos <- data.frame(lapply(datos, trimws))
datos2 <- subset(datos, Comunidad == "alccasana")

library(wordcloud2)
library(tm)
library(webshot)

datos2$Turba[datos2$Turba == "se secan"] <- "se seca"
datos2$Turba[datos2$Turba == "se esta secando"] <- "se seca"
datos2$Turba[datos2$Turba == "ninguno"] <- NA
datos2$Turba[datos2$Turba == "no sabe"] <- NA

#datos2$Vegper[datos2$Vegper == "pato"] <- NA
#datos2$Vegper[datos2$Vegper == "chalhua"] <- NA

noNaa <- datos2$Turba[!is.na(datos2$Turba)]
frecuencias <- table(noNaa)
graph18 <- wordcloud2(data = frecuencias, size = 1.5)
graph18
@
\caption{Alteraciones de la turba en la qocha}
\end{figure}
Se observa que la turba se esta secando y desapareciendo, esto debido a la falta de agua, la turba es un compuesto de materiales organicos que es utilizada en la agricultura.\\
